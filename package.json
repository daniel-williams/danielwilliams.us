{
  "name": "react-mobx-nimble",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Nimble React starter using JSX, Typescript, Webpack 3 and MobX for state management.",
  "author": {
    "name": "Daniel James Williams",
    "email": "mail@danielwilliams.us"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-williams/react-mobx-nimble"
  },
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production npm run webpack-build",
    "dev": "cross-env NODE_ENV=dev concurrently --kill-others-on-fail \"npm run start:api\" \"npm run webpack-dev-server\"",
    "start": "npm run dev",
    "start:api": "node ./server",
    "webpack-build": "rimraf dist && webpack --config config/webpack.build.js --progress --profile --bail",
    "webpack-dev-server": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base client/"
  },
  "keywords": [
    "boilerplate",
    "MobX",
    "nimble",
    "React",
    "seed",
    "starter",
    "todo",
    "Typescript",
    "Webpack"
  ],
  "main": "index.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "gsap": "^1.20.3",
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "mobx-react-router": "^4.0.2",
    "node-fetch": "^2.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-measure": "^2.0.2",
    "react-motion": "^0.5.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^1.2.1",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/express": "^4.11.1",
    "@types/gsap": "^1.19.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^8.0.46",
    "@types/node-fetch": "^1.6.9",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-motion": "0.0.26",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-transition-group": "^1.1.3",
    "@types/webpack": "^3.0.13",
    "awesome-typescript-loader": "^3.2.3",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.2.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "mobx-logger": "^0.7.0",
    "mobx-react-devtools": "^5.0.1",
    "node-sass": "^4.6.1",
    "postcss": "^6.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "to-string-loader": "^1.1.5",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
